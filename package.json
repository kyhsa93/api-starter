{
  "name": "express-api-starter",
  "version": "1.0.0",
  "description": "express api starer kit",
  "main": "index.js",
  "scripts": {
    "doc": "jsdoc -c jsdoc.conf",
    "lint": "eslint --fix index.js config.js src test && npm run doc",
    "build": "npm run lint && babel index.js -d lib && babel config.js -d lib && babel src -d lib/src && babel test -d lib/test",
    "watch": "npm run lint && babel-watch --watch src index.js config.js",
    "test": "npm run build && istanbul cover _mocha ./lib/test/",
    "start": "npm run build && node ./lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyhsa93/express-api-starter.git"
  },
  "keywords": [
    "express",
    "babel",
    "eslint",
    "jsdoc",
    "mocha",
    "chai",
    "istanbul"
  ],
  "author": "kyhsa93",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kyhsa93/express-api-starter/issues"
  },
  "homepage": "https://github.com/kyhsa93/express-api-starter#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "babel-watch": "^2.0.7",
    "chai": "^4.1.2",
    "eslint-plugin-mocha": "^5.0.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "jsdoc-route-plugin": "^0.1.0",
    "mocha": "^5.0.5"
  }
}
